<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <!-- <h1 class="m-0 font-weight-bold text-primary mt-4">Image</h1> -->
  <!-- DataTales Example -->
  <div class="card shadow mb-4">
    <div
      class="card-header py-3 d-flex justify-content-between align-items-center"
    >
      <h6 class="m-0 font-weight-bold text-primary">Detail Image</h6>
      <a href="/admin/item" class="btn-sm btn-dark p-2">Back To Item</a>
    </div>
    <div class="card-body p-2">
      <div class="row">
        <% for(let i = 0; i < item.imageId.length; i++) {%>
        <div class="col-md-4">
          <img
            src="/<%= item.imageId[i].imageUrl %>"
            alt="foto produk"
            style="width: 100%; display: block"
          />
        </div>
        <% } %>
      </div>
    </div>
  </div>
</div>
